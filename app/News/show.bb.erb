<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @news.en_news %></h1>
    <%= link_to "Edit", :action => :edit, :id => @news.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">EnNews</td>
        <td class="itemValue"><%= @news.en_news%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">JpNews</td>
        <td class="itemValue"><%= @news.jp_news%></td>
      </tr>
      
    </table>
  </div>

</div>
